{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "firefox",
			"request": "launch",
			"name": "vuejs: firefox",
			"url": "http://localhost:6060",
			"webRoot": "${workspaceFolder}/src",

			"pathMappings": [
				{
					"url": "webpack:///src/",
					"path": "${webRoot}/"
				}
			]
		}
	]
}
